	background-color: var(--login-background);
	text-align: center;
	padding: 5rem 0;
	margin-top: 5rem;
}

.password-change p {
	width: 80px;
	margin-bottom: 0;
	text-align: left;
}

.web-modal-title {
	margin-bottom: 1rem;
}

.web-modal-body {
	padding: 2rem 5rem 5rem;
}

.rank-box {
	display: grid;
	grid-template-columns: repeat(5, 1fr);
	gap: 20px;
	min-height: 400px;
}

.rank-each-box {
	border: 1px solid var(--input-border);
	border-radius: 5px;
	height: 100%;
	padding: 10px;
}

.rank-each-box img {
	margin: 1rem 0;
}

.rank-title {
	text-align: center;
}

.rank-title p {
	margin: 2rem 0;
}

.rank-each-box ul {
	padding-left: 1rem;
}

.rank-each-box ul li {
	position: relative;
	padding-left: 15px;
	color: #787878;
}

.rank-each-box ul li:before {
	content: "";
	width: 3px;
	height: 3px;
	background-color: var(--color7);
	display: inline-block;
	position: absolute;
	top: 12px;
	left: 0;
	transform: translate(-50%, -50%);
}

.rank-li-title{
	padding-left: 10px;
	margin-bottom: 0.5rem;
	font-weight: 700;
}

@media screen and (max-width: 1000px) {
	.room-intro {
		display: block;
	}

	.room-intro .room-image {
		width: 100%;
	}


}