}

.accessibility_BY #footer_brand_menu_wrap .apps a {
    filter: grayscale(0) contrast(2)
}

#footer_brand_menu_wrap .apps a.appstore {
    margin-left: 10px
}

#footer_brand_menu {
    -moz-flex-grow: 1;
    flex-grow: 1
}

#footer_brand_menu .footer_menu {
    display: -moz-flex;
    display: flex;
    -moz-align-items: center;
    align-items: center
}

@media (max-width: 767px) {
    #footer_brand_menu .footer_menu {
        display:block
    }
}

#footer_brand_menu .footer_menu>li>a {
