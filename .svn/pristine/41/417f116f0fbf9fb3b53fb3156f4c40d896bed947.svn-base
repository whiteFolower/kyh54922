/*  보관함 */
.status-ongoing,
.status-completed,
.status-rejected,
.status-unknown {
    display: inline-block;
    padding: 5px 10px;
    border-radius: 15px;
    font-weight: bold;
    text-align: center;
    min-width: 80px;
}

.status-ongoing {
    color: #007bff;
    background-color: #cce5ff;
    border: 1px solid #b8daff;
}

.status-completed {
    color: #28a745;
    background-color: #d4edda;
    border: 1px solid #c3e6cb;
}

.status-rejected {
    color: #dc3545;
    background-color: #f8d7da;
    border: 1px solid #f5c6cb;
}

.status-unknown {
    color: #6c757d;
    background-color: #e9ecef;
    border: 1px solid #dee2e6;
}




