.main-panel>.appsidebar {
	display: flex;
}

.asidebar {
	width: 250px;
	border-right: 1px solid var(--primary-calendar-cell-range-bg);
    padding-top: 2rem;
}

.appro-funtion{
	justify-content: center;
}

.appro-txt{
	padding: 13px;
    display: inline-block;
    border: 1px solid #777;
}

.pbot-4 {
	padding-bottom: 1rem;
}

.asidebar .nav-item {
	width: 100%;
}


.asidebar .nav-collapse{
	display: block;
	margin-top: 0;
    margin-bottom: 15px;
    padding-bottom: 15px;
    padding-top: 10px;
}

.asidebar .nav-collapse li a{
	margin-bottom: 10px;
    padding: 10px 25px;

}

.asidebar .nav-collapse li a .sub-item{
	margin-left: 25px;
	color: var(--color4);
	position: relative;
}

.asidebar .nav>.nav-item a {
	display: flex;
	align-items: center;
	width: 100%;
	color: var(--font-primary);
/* 	justify-content: space-between; */
    position: relative;
	padding: 5px 35px;
}
.asidebar .nav-item>a p{
	margin-bottom : 0;
}

.asidebar .nav-item.submenu {
	background: rgba(0, 0, 0, .03);
}

.asidebar .nav-item .caret{
    margin-left: auto;
    margin-right: 6px;
    transition: all .5s;
    color: #8d9498;
}

.nav>.nav-item a i{
    color: #8d9498;
    margin-right: 15px;
    width: 25px;
    text-align: center;
    vertical-align: middle;
    float: left;
    font-size: 16px;
    line-height: 30px;
}
.content_tab_wrap {
    height: 300px;
    /* border: 0; */
    overflow: auto;
    padding: 8px 0;
     border: 0;
    border-top: 1px solid #ddd;
}

.content_tab_wrap li{
	margin: 10px;
}

.form-group{
	padding: 0;
}

.fa-regular{
	margin-right: 10px;
}



/*approform.jsp  */
.dashboard_box {
    min-height: 20px;
    padding: 12px 24px;
}

.card_flex .approval_home_card {
/*     width: 25%; */
    margin: 0 0 0 0;
}

.card_flex .card_item {
    display: table-cell;
    width: 350px;
    float: none;
}

.card_flex .card_item .card_wrapper {
    position: relative;
    border: 1px solid #ddd;
    border-radius: 8px;
    background: #fff;
    overflow: hidden;
}

.card_flex .card_item .card_wrapper {
    margin: 0 16px 0 0;
}

.card_item .card_wrapper {
    transition: 0.3s;
}

.card_item div.card_wrapper header {
    padding: 12px 16px 0;
    border-top: 5px solid #fff;
    border-radius: 6px;
}

.card_item div.card_wrapper span.state {
    padding: 4px;
    margin-right: 5px;
    width: auto;
    font-size: 12px;
}

span.state.ongoing {
    background-color: #a6c76c;
    border: 1px solid #a6c76c;
}

span.state {
    display: inline-block;
    padding: 4px;
    text-align: center;
    color: #fff;
    border-radius: 2px;
    letter-spacing: -1px;
    line-height: 11px;
    font-size: 12px;
    vertical-align: middle;
}

.card_item div.card_wrapper .card_subject {
    padding-bottom: 0;
}

.card_item div.card_wrapper .card_subject, .card_item div.card_wrapper .card_content {
    padding: 0 16px 16px 16px;
}

.card_flex .approval_home_card .card_wrapper .t_card {
    white-space: normal;
    height: 28px;
    max-width: 80%;
}

.card_item div.card_wrapper span.t_card {
    display: inline-block;
}

.card_item div.card_wrapper span.title {
    display: inline-block;
    line-height: 1.4;
    font-size: 19px;
    color: #484848;
    letter-spacing: -1px;
    margin: 6px 0 0;
    font-weight: bold;
    font-weight: 500;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.approval_home_card .card_content {
    min-height: 130px
}

.form {
    display: table;
    width: 100%;
    table-layout: fixed;
}

.form .form_tr {
    display: table-row;
}

.card_item .form .form_tr .form_th {
    width: 60px;
    padding: 2px 0;
    font-size: 13px;
    color: #888;
}

.form .form_tr .form_th {
    display: table-cell;
    vertical-align: top;
    width: 80px;
}

.card_item .form .form_tr .form_td {
    padding: 2px 0;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    max-width: 100%;
    font-size: 13px;
    color: #888;
}

.form .form_tr .form_td {
    display: table-cell;
}

.dashboard_box .card_item .card_action {
    margin: 0;
    height: 35px;
}

.card_action {
    margin: 50px 10px 0px 10px;
    text-align: center;
}

.dashboard_box .card_item .card_action .btn_lead {
    font-weight: bold;
    font-weight: 500;
}

.dashboard_box .card_item .card_action .btn_lead, .gadget_design_wrap .card_item .card_action .btn_lead {
    width: 100%;
    padding: 12px 0;
    border: 0;
    background: none;
    color: #333;
    font-size: 13px;
    border-top: 1px solid #ebebeb;
}

span.txt {
    display: inline-block;
}

.search_wrap {
	display : flex;
    position: relative;
    border: 1px solid #ddd;
}

.search_wrap input.btn_search {
    display: inline-block;
    cursor: pointer;
    height: 100%;
    position: absolute;
    right: 0;
    top: 0;
    width: 31px;
    padding: 5px 0px;
    border-radius: 0;
    font-weight: bold;
    font-weight: 500;
}

section.search div.search_wrap input {
    padding: 0 4px;
    height: 35px;
}


/* 결재선 버튼라인  */
.tool_bar {
    position: relative;
    min-height: 38px;
    padding: 0 0 8px 0;
}

.report_detail .tool_bar .critical {
    height: 32px;
}

.tool_bar .critical {
    float: left;
    margin: 6px 0 0 24px;
}

.tool_bar .optional {
    float: right;
    margin: 5px 24px 0 0;
}

.tool_bar>ul {
    min-height: 30px;
}

.tool_bar ul.optional li {
    position: relative;
}

.tool_bar>ul>li {
    float: left;
}


.btn_tool {
    float: left;
    display: inline-block;
    height: 30px;
    line-height: 29px;
    padding: 0 8px;
    white-space: nowrap;

}





/*기안 입력부분  */
.anntable {
	margin: 12px;
	padding: 10px;
    border: 5px solid #e8e8e8;
    padding-bottom: 20px;
}

.headerline{
	border-top: 1px solid #ddd;
}

.cal {
	 text-align: start;
}

.cal > input[type='date']{
  width: 20%;
}

.halfArea >input{
	width: 5%;
}




/*파일부분  */
.file{
	padding-top: 50px;
    width: 80%;

}

table.form_type tbody th {
    text-align: left;
    vertical-align: top;
}

.dd_attach {
    position: relative;
    overflow-y: auto;
    padding: 8px 0;
    border: 2px dashed #ddd;
    box-sizing: border-box;
    max-height: 300px;
    font-size: 13px;
}
.dd_attach .area_txt {
    padding: 8px 0;
    text-align: center;
}

.dd_attach .area_txt .msg .btn_file {
    position: relative;
    display: inline-block;
    width: 48px;
    cursor: pointer;
    text-align: left;
    overflow: hidden;
    vertical-align: middle;
    margin: -4px 0 0 2px;
}

.dd_attach .area_txt .msg .btn_file .txt {
    display: inline-block;
    color: #666;
    cursor: pointer;
    text-decoration: underline;
}

.dd_attach .area_txt .msg .btn_file input[type="file"] {
    display: inline-block;
    position: absolute;
    top: 0;
    left: 0;
    opacity: 0;
    cursor: pointer;
}




/*결재선  */
.sign_type1_inline {
    position: relative;
    display: inline-table;
/*     border-collapse: separate; */
    border-spacing: 0;
    white-space: normal;
    font-size: 12px;
    margin: 1px -1px 0 0;
    padding-left: 26px;
}

.sign_type1_inline .sign_tit_wrap {
/*     position: absolute; */
    top: -1px;
    left: 0;
    bottom: 0;
    vertical-align: middle;
    border: 1px solid #000;
    width: 24px;
    background: #eee;
    display: table-cell;
}

.sign_type1_inline .sign_tit_wrap .sign_tit {
    display: table;
    padding: 0 2px;
    text-align: center;
    word-break: break-all;
    white-space: normal;
    height: 100%;
}

.sign_type1_inline .sign_tit_wrap .sign_tit strong {
    display: table-cell;
    vertical-align: middle;
}

.sign_type1_inline .sign_member_wrap {
    vertical-align: top;
    background: #fff;
    display: table-cell;
/*     float: left; */
    border: 1px solid #000;
    border-left: 0;
/*     margin: -1px 0 0 0; */
}


.sign_type1_inline .sign_member_wrap .sign_member {
    width: 79px;
    vertical-align: top;
    display: table;
    table-layout: fixed;
}

.sign_type1_inline .sign_member_wrap .sign_member .sign_rank_wrap, .sign_type1_inline .sign_member_wrap .sign_member .sign_date_wrap {
    display: table-row;
}


.sign_type1_inline .sign_member_wrap .sign_member .sign_posi {
    height: 26px;
}

.sign_type1_inline .sign_member_wrap .sign_member .sign_wrap {
    height: 75px;
    border-top: 1px solid #000;
}

.sign_type1_inline .sign_member_wrap .sign_member .sign_wrap > span {
    padding: 2px 0;
    display: block;
}

.sign_type1_inline .sign_member_wrap .sign_member .sign_date {
    height: 26px;
    border-top: 1px solid #000;
}

.sign_type1_inline .sign_member_wrap .sign_member .sign_posi,
.sign_type1_inline .sign_member_wrap .sign_member .sign_wrap,
.sign_type1_inline .sign_member_wrap .sign_member .sign_date {
    display: table-cell;
    vertical-align: middle;
    text-align: center;
    width: 79px;
}






/* 결재선 모달 */

ul{
	list-style: none;
}

div.tab_menu_wrap {
    margin: 15px 0 15px;
    padding: 0;
    display: block;
    border-bottom: 1px solid #dfdfdf;
}

ul.tab_menu li:first-child {
    margin-left: 0;
}

ul.tab_menu {
    overflow: hidden;
    margin-bottom: -1px;
}
ul.tab_menu li {
    float: left;
    padding: 12px 0;
    margin: 0 16px;
    z-index: 1;
    cursor: pointer;
}
ul.tab_menu li.active, ul.tab_menu li.ui-state-active {
    border-bottom: 2px solid #000;
}

ul.tab_menu li span.ic_txt_change {
    display: none;
}

ul.tab_menu li.active span.txt, ul.tab_menu li.ui-state-active span.txt {
    color: #333;
    font-weight: bold;
    font-weight: 500;
}






